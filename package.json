{
  "name": "bookstore",
  "version": "1.0.0",
  "description": "A simple RESTful API using Node.js and tested with Mocha and Chai",
  "main": "server.js",
  "repository": "git@github.com:marks2508/bookstore.git",
  "author": "Mark S",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && node server",
    "start:client": "webpack-dev-server",
    "start:server": "nodemon",
    "build": "webpack -p",
    "seed": "node db/seeds",
    "test:client": "nyc --reporter=text --reporter=html --all mocha test/client/helper.js \"test/client/**/*.js\"",
    "test:server": "nyc --reporter=text --reporter=html --all mocha test/api/**/*.js --exit"
  },
  "dependencies": {
    "axios": "0.16.2",
    "body-parser": "^1.15.1",
    "express": "^4.13.4",
    "jsonwebtoken": "^8.3.0",
    "materialize-css": "^1.0.0-rc.2",
    "mongodb": "2.2.33",
    "mongoose": "^4.4.15",
    "mongoose-unique-validator": "1.0.6",
    "morgan": "^1.7.0",
    "node": "^10.8.0",
    "nodemon": "^1.18.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "supertest": "^3.3.0"
  },
  "devDependencies": {
    "atob": "2.0.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "11.6.2",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.3",
    "nyc": "^13.0.1",
    "sass-loader": "^7.1.0",
    "sinon": "4.4.2",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "3.3.0",
    "webpack-dev-server": "2.5.1"
  }
